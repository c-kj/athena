{
    "configurations": [
        {
            "name": "Athena++ Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/homebrew/opt/fftw/include",
                "/opt/homebrew/opt/hdf5/include"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "macos-clang-arm64",
            "compilerPath": "/opt/homebrew/opt/llvm/bin/clang++",
            "compilerArgs": [
                "-g",
                "-O3",
                "-march=native"
            ]
        },
        {
            "name": "Athena++ wm2",
            "includePath": [
                "${workspaceFolder}/src",
                "/lustre/software/oneapi/2023.0/mpi/2021.8.0/include",
                "/lustre/software/fftw/3.3.10/gcc_8.5.0/include",
                "/lustre/software/hdf5/1.12.1-p/oneapi_2023.0/include"
            ],
            "defines": [],
            "compilerPath": "/lustre/software/oneapi/2023.0/compiler/2023.0.0/linux/bin/icpx",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "${default}",
            "compilerArgs": [
                "-g",
                "-O3",
                "-xhost",
                "-flto",
                "-ipo",
                "-qopenmp-simd",
                "-funroll-loops",
                "-fast"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/src"
                ]
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}